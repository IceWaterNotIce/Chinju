.time-control-bar {
    background-color: rgb(46, 46, 46);
    padding: 10px;
    border-radius: 5px;
    align-items: auto;
    flex-direction: row-reverse;
    align-content: auto;
    right: auto;
    flex-grow: 0;
}

.time-control-btn {
    background-color: rgba(255, 255, 255, 0.1);
    border-width: 2px;
    border-color: rgba(255, 255, 255, 0.3);
    border-radius: 5px;
    color: white;
}